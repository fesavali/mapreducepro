{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Launch mapreducepro",
            "request": "launch",
            "mainClass": "mapreducepro.mapreducepro",
            "projectName": "mapreducepro"
        },
        {
            "command": "npm start",
            "name": "Run npm start",
            "request": "launch",
            "type": "node-terminal"
        },
        {
            "name": "Launch Edge",
            "request": "launch",
            "port": 12651,
            "restart": true,
            "type": "pwa-msedge",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}"
        },
        {
            "type": "java",
            "name": "Launch Java Program",
            "request": "launch",
            "mainClass": "main.java"
        },
        {
            "name": "Attach to Edge",
            "port": 12651,
            "request": "attach",
            "type": "pwa-msedge",
            "restart": true,
            "url": "http://localhost:8080/",
            "webRoot": "${workspaceFolder}"
        },
        {
            "type": "java",
            "name": "Launch Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Launch main",
            "request": "launch",
            "mainClass": "mapreducepro.main",
            "projectName": "mapreducepro"
        }
    ]
}